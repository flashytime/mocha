{
    "name": "flashytime/mocha",
    "description": "Mocha is a lightweight and high-performance PHP framework for web applications.",
    "keywords": [
        "mocha",
        "php",
        "framework"
    ],
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
            "name": "flashytime",
            "email": "myflashytime@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "monolog/monolog": "^1.23",
        "filp/whoops": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Mocha\\App\\": "app/",
            "Mocha\\Framework\\": "framework/"
        },
        "files": [
          "framework/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mocha\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
